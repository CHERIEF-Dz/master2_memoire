\series bold
mauvaise
\series default
 planification peut entraîner:
\end_layout

\begin_layout Itemize
une 
\series bold
baisse des performances
\series default
 du système
\end_layout

\begin_layout Itemize
une 
\series bold
longue période
\series default
 de rétroaction des demandes
\end_layout

\begin_layout Itemize
une 
\series bold
baisse de la satisfaction
\series default
 des utilisateurs
\end_layout

\end_deeper
